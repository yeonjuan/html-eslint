{
  "name": "@html-eslint/eslint-plugin-svelte",
  "version": "0.57.1",
  "type": "module",
  "description": "ESLint plugin for HTML with Svelte support",
  "author": "yeonjuan",
  "homepage": "https://github.com/yeonjuan/html-eslint#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.js",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeonjuan/html-eslint.git"
  },
  "scripts": {
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "ts": "tsc --noEmit",
    "lint": "eslint .",
    "build": "tsup"
  },
  "bugs": {
    "url": "https://github.com/yeonjuan/html-eslint/issues"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "html",
    "svelte",
    "lint"
  ],
  "dependencies": {
    "@html-eslint/core": "^0.57.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0 || ^10.0.0-0",
    "svelte-eslint-parser": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@html-eslint/types": "^0.57.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint-scope": "^9.1.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@typescript-eslint/types": "^8.56.0",
    "babel-jest": "^30.2.0",
    "eslint-scope": "^9.1.1",
    "jest": "^30.2.0",
    "svelte": "^5.53.1",
    "svelte-eslint-parser": "^1.5.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  }
}
