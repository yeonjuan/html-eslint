{
  "name": "@html-eslint/eslint-plugin-angular-template",
  "version": "0.57.0",
  "type": "commonjs",
  "description": "ESLint plugin for HTML with Angular support",
  "author": "yeonjuan",
  "homepage": "https://github.com/yeonjuan/html-eslint#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeonjuan/html-eslint.git"
  },
  "scripts": {
    "test": "jest --coverage",
