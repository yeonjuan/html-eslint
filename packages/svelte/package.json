{
  "name": "@html-eslint/svelte",
  "version": "0.1.0",
  "type": "module",
  "description": "HTML ESLint plugin for Svelte",
  "author": "yeonjuan",
  "homepage": "https://github.com/yeonjuan/html-eslint#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "types/exports.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yeonjuan/html-eslint.git"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.mjs --coverage",
    "ts": "tsc --noEmit",
    "lint": "eslint .",
    "build": "rimraf types && tsc --project tsconfig.build.json"
  },
  "bugs": {
    "url": "https://github.com/yeonjuan/html-eslint/issues"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "html",
    "svelte",
    "lint",
    "accessibility"
  ],
  "dependencies": {
    "@html-eslint/core": "^0.1.0",
    "@html-eslint/types": "^0.55.0",
    "html-standard": "0.0.12"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0 || ^10.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@types/eslint-scope": "^8.4.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@typescript-eslint/types": "^8.55.0",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "svelte": "^5.17.1",
    "svelte-eslint-parser": "^1.4.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  }
}
