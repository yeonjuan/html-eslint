{
  "name": "website",
  "version": "0.40.3",
  "private": true,
  "scripts": {
    "clear:cache": "rimraf ../../parcel-cache",
    "dev": "yarn clear:cache && yarn docs && parcel ./src/index.html -p 3000 --open",
    "build": "yarn docs && parcel build ./src/index.html --dist-dir out && yarn sitemap",
    "lint": "eslint src",
    "docs": "node ./scripts/docs",
    "deploy": "yarn build && firebase deploy",
    "sitemap": "node ./scripts/sitemap"
  },
  "dependencies": {
    "@html-eslint/eslint-plugin": "^0.40.3",
    "@html-eslint/parser": "^0.40.0",
    "@html-eslint/web-linter": "^0.40.0",
    "@html-kit/html": "^0.0.4",
    "codemirror": "^5.58.3"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.13.0",
    "@stylistic/eslint-plugin": "^3.0.0",
    "@types/codemirror": "^5.60.5",
    "@types/eslint": "^9.6.1",
    "@types/node": "17.0.23",
    "@types/react": "^18.0.26",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.13",
    "eslint": "^9.19.0",
    "firebase-tools": "^14.3.1",
    "highlight.js": "^11.9.0",
    "marked": "^9.1.5",
    "marked-highlight": "^2.0.6",
    "parcel": "^2.15.0",
    "postcss": "^8.4.19",
    "posthtml-extend": "^0.6.5",
    "posthtml-include": "^1.7.4",
    "posthtml-modules": "^0.9.1",
    "process": "0.11.10",
    "rimraf": "^5.0.5",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.3.5"
  },
  "@parcel/transformer-js": {
    "inlineFS": false,
    "inlineEnvironment": false
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
}
