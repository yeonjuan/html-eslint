  "peerDependencies": {
    "eslint": ">=8.0.0 || ^10.0.0-0"
  },
  "devDependencies": {
    "@angular-eslint/bundled-angular-compiler": "^21.2.0",
    "@angular-eslint/template-parser": "^19.1.1",
    "@angular/compiler": "^21.1.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "typescript": "^5.8.3"
