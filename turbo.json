{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "lint": {},
    "dev": {
      "dependsOn": ["^dev", "^build"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "test:legacy": {
      "dependsOn": ["^build"]
    },
    "ts": {
      "dependsOn": ["^build"]
    },
    "test:integration": {
      "dependsOn": ["@html-eslint/eslint-plugin#build", "^build"]
    },
    "docs": {}
  }
}
